<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloworld" class="com.example.HelloWorld" init-method="init" destroy-method="destroy"/>

    <bean id="byjavacode" class="com.example.HelloWorldByJavaCode"/>

    <bean id="byannotations" class="com.example.HelloWorldByAnnotations"/>
    <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor"/>

</beans>